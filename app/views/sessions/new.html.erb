<div class="session-container">
<h1>Log In</h1>
<br><br>
  <%= form_tag sessions_url do %>
    <div class="field">
      <%= label_tag :email %> <br>
      <%= text_field_tag :email %>
    </div>
    <div class="field">
      <%= label_tag :password %> <br>
      <%= password_field_tag :password %>
    </div>
    <div class="action">
      <%= submit_tag "Login" %>
    </div>
    <% end %>
</div>
